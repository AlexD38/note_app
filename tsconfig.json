{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/(routes)/file/page.jsx", "app/(routes)/file/page.jsx", "app/@auth/login/page.js", "app/ui/add-note-modal/page.js", "app/ui/login_modal/page.js", "app/(routes)/file/page.jsx", "app/ui/folder_navbar/page.jsx", "app/ui/folder_files_list/page.jsx", "app/ui/login_modal/page.jsx", "app/ui/signup_modal/page.jsx", "app/ui/To_sort_files/toSortFiles.jsx", "app/(routes)/users/page.js", "app/lib/data.js"],
  "exclude": ["node_modules"]
}
